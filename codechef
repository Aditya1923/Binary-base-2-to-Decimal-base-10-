import java.util.*;
public class Binary_to_Decimal{
public static void main(String args[]){
Scanner sc=new Scanner(System.in);
int n,c=0;
double d=0.0,r=0.0;
System.out.println("Enter the binary digits");
n=sc.nextInt();
while(n>0){
r=n%10;
d=d+r*Math.pow(2,c);
n=n/10;
c+=1;
}
System.out.println((int)d);
}
}
